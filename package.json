{
  "name": "molecule",
  "version": "0.2.1",
  "description": "Modern starter kit for React, Redux & Electron Apps.",
  "author": "Zach Sherman <zach@timber.io>",
  "license": "MIT",
  "scripts": {
    "serve:src": "cross-env NODE_ENV=development webpack-dashboard -- babel-node ./tools/srcServer.js",
    "serve:dist": "cross-env NODE_ENV=production babel-node ./tools/distServer.js",
    "clean:dist": "rimraf ./dist",
    "lint": "eslint ./src ./tools ./webpackConfig \"./*.js\"",
    "test": "mocha --opts ./mocha.opts \"./{,!(node_modules)/**/}*.test.js\"",
    "test:watch": "npm run test -- --watch",
    "predev": "babel-node ./tools/startMessage.js",
    "dev": "NODE_ENV=development npm run build:dll && run-p serve:src",
    "prebuild": "run-s clean:dist",
    "build:dll": "babel-node ./tools/dll.js",
    "build": "cross-env && NODE_ENV=production babel-node ./tools/build.js",
    "start": "npm run serve:dist",
    "browsers": "babel-node ./tools/printBrowsersList"
  },
  "dependencies": {
    "babel-plugin-react-css-modules": "2.2.0",
    "babel-polyfill": "^6.23.0",
    "classnames": "2.2.5",
    "immutable": "^3.8.1",
    "prop-types": "^15.5.10",
    "react": "^15.5.4",
    "react-dom": "^15.5.4",
    "react-hot-loader": "^3.0.0-beta.6",
    "react-redux": "^5.0.4",
    "react-router": "3.0.0",
    "react-router-redux": "4.0.8",
    "redux": "3.6.0",
    "redux-immutable": "^4.0.0",
    "redux-saga": "^0.15.3",
    "reselect": "^3.0.1"
  },
  "devDependencies": {
    "add-asset-html-webpack-plugin": "^2.0.1",
    "autoprefixer": "6.7.7",
    "babel-cli": "6.23.0",
    "babel-core": "6.23.1",
    "babel-eslint": "7.1.1",
    "babel-loader": "6.4.0",
    "babel-preset-latest": "6.22.0",
    "babel-preset-react": "6.23.0",
    "babel-preset-stage-3": "6.22.0",
    "babel-register": "6.23.0",
    "browser-sync": "2.18.8",
    "chai": "3.5.0",
    "chai-enzyme": "0.6.1",
    "chalk": "1.1.3",
    "circular-dependency-plugin": "^3.0.0",
    "connect-history-api-fallback": "1.3.0",
    "cross-env": "3.2.3",
    "css-loader": "0.27.2",
    "dotenv": "^4.0.0",
    "enzyme": "2.7.1",
    "eslint": "^3.19.0",
    "eslint-config-airbnb": "14.1.0",
    "eslint-import-resolver-webpack": "0.8.1",
    "eslint-loader": "1.6.3",
    "eslint-plugin-import": "2.2.0",
    "eslint-plugin-jsx-a11y": "4.0.0",
    "eslint-plugin-react": "6.9.0",
    "extract-text-webpack-plugin": "2.1.0",
    "file-loader": "0.10.1",
    "html-webpack-plugin": "2.28.0",
    "imagemin-webpack-plugin": "1.4.4",
    "jsdom": "9.12.0",
    "lost": "8.0.0",
    "mocha": "3.2.0",
    "npm-run-all": "4.0.2",
    "postcss-loader": "1.3.3",
    "react-addons-test-utils": "15.4.2",
    "react-dev-utils": "^0.5.2",
    "react-svg-loader": "2.0.0-alpha.1",
    "rimraf": "2.6.1",
    "sinon": "1.17.7",
    "sinon-chai": "2.8.0",
    "style-loader": "0.13.2",
    "stylelint-config-standard": "15.0.1",
    "stylelint-webpack-plugin": "0.7.0",
    "svg-url-loader": "2.0.2",
    "url-loader": "0.5.8",
    "webpack": "^2.5.1",
    "webpack-bundle-analyzer": "^2.8.1",
    "webpack-dashboard": "^0.4.0",
    "webpack-dev-middleware": "1.10.1",
    "webpack-hot-middleware": "2.17.1"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/timberio/molecule.git"
  }
}
